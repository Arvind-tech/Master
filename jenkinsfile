pipeline {
       agent { label "slave1" }
             stages{
                  stage('scm checkout'){
	                  steps{
                        script{
                             git checkout 'https://github.com/Arvind-tech/Master.git'
                           }
                         }
                     }
                  stage('maven version'){
                      steps{
                        script{
                          sh "mvn --version" 
                          }
                         }
                   }
}
}
